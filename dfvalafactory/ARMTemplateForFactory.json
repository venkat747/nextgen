{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfvalafactory"
		},
		"AzureSqlDatabase1_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1_connection'"
		},
		"AzureBlobStorage1_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1_connection'"
		},
		"AzureDataLakeStorage1_connection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://csgc3636551e5ecx41fcxbea.dfs.core.windows.net"
		},
		"AzureDataLakeStore1_connection_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://lakestoredata.azuredatalakestore.net/webhdfs/v1"
		},
		"AzureDataLakeStore1_connection_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "63ce7d59-2f3e-42cd-a8cc-be764cff5eb6"
		},
		"AzureDataLakeStore1_connection_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "c3636551-e5ec-41fc-bea7-4750d9b17835"
		},
		"AzureDataLakeStore1_connection_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "BasicRG"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureSqlDatabase1_connection')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('AzureSqlDatabase1_connection_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStorage1_connection')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('AzureDataLakeStorage1_connection_properties_typeProperties_url')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStore1_connection')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureDataLakeStore",
				"typeProperties": {
					"dataLakeStoreUri": "[parameters('AzureDataLakeStore1_connection_properties_typeProperties_dataLakeStoreUri')]",
					"tenant": "[parameters('AzureDataLakeStore1_connection_properties_typeProperties_tenant')]",
					"subscriptionId": "[parameters('AzureDataLakeStore1_connection_properties_typeProperties_subscriptionId')]",
					"resourceGroupName": "[parameters('AzureDataLakeStore1_connection_properties_typeProperties_resourceGroupName')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobStorage1_connection')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('AzureBlobStorage1_connection_connectionString')]"
				}
			},
			"dependsOn": []
		}
	]
}